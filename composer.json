{
    "name": "atg-ltd/codeception-phpbuiltinserver",
    "description": "PhpBuiltinServer extension for Codeception",
    "keywords": ["codeception"],
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "ATG",
            "email": "tools.services@atg.auto"
        }
    ],
    "require": {
        "php": ">=8.0",
        "codeception/codeception": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "10.*",
        "codeception/module-phpbrowser": "^3.0"
    },
    "autoload": {
        "psr-0": {
            "Codeception": "src"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "replace": {
        "codeception/phpbuiltinserver": "^1.5"
    }
}
